<h1>users list</h1>
<table border="1">
	<tr>
		<th>Fname</th>
		<th>Lname</th>
		<th>Email</th>
		<th>password</th>
		<th>Edit</th>
		<th>Show</th>
		<th>Delete</th>
	</tr>
	<% @users.each do|user| %>
	<tr>
		<td><%= user.fname %></td>
		<td><%= user.lname %></td>
		<td><%= user.email %></td>
		<td><%= user.password %></td>
		<td><%= link_to "edit", edit_user_path(user) %></td>
		<td><%= link_to "show", user_path(user)%></td>
		<td><%= link_to "delete", user_path(user), :method=>:delete %></td>
	</tr>
	<% end %>
</table></br>
<%= link_to 'new user',new_user_path %>